{
  "name": "JiraRestClient.NETCore",
  "version": "1.0.5",
  "title": "JiraRestClient for .NetCore",
  "description": "A simple RestClient for Jira-RestAPI, using only .NetCore Modules.",
  "copyright": "cschulc 2016",
  "packOptions": {
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
    "repository": {
      "url": "https://github.com/cschulc/JiraRestClient.NETCore",
      "type": "git"
    }
  },
  "authors": [
    "Christian Schulze, cschulc@outlook.com"
  ],
  "language": "en_US",
  "buildOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.3"
    },
    "System.Net.Http": "4.3.0",
    "System.Runtime.Serialization.Json": "4.3.0",
    "System.Runtime.Serialization.Primitives": "4.3.0",
    "xunit": "2.1.0",
    "dotnet-test-xunit": "1.0.0-rc2-build10025"
  },
  "testRunner": "xunit",
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  }
}